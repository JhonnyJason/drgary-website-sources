#ordination-container
    margin-content(20px)
    margin-top: 30px
    margin-bottom: 70px
    p   
        line-height: 1.2
        font-size: 20px
        text-align: center
        margin-top: 30px
        margin-bottom: 0
        @media desktopscreen
            width: 500px
            margin-left: auto
            margin-right: auto
    p.top-line
        display: block
        margin: auto
        font-size: 23px
        text-align: center
        margin-top: 50px
        margin-bottom: 40px
        padding-bottom: 20px
        border-bottom: solid 1px garyred
        @media desktopscreen
            width: calc(100% - 320px)
            font-size: 27px
            padding-left: 160px
            padding-right: 140px
            
    .spacer
        height: 40px
        width: 100%
        centered-background("/img/logo.svg")
        margin: 30px auto
    #consent-block
        display: none
        position absolute
        top 50%
        left 50%
        transform translate(-50%, -50%)
        background-color white
        border solid 2px garyred
        padding 20px
        box-sizing: border-box
        h5
            text-align: center
            margin-bottom 10px
        p
            font-size 14px
            margin-bottom  5px
            margin-top 5px

        button
            background-color garyred
            color white
            font-size: 14px
            font-weight: bold
            padding 10px 20px

    #google-maps-frame
        position relative
        width: 100%
        height: 400px
        margin-top: 60px
        background-color #4d4d4d
    button
        display: block 
        margin: auto
        margin-top: 30px
    a
        color: #2e2e2e
        word-break: break-all
#imageGallery
    display: flex
    justify-content: space-around
    flex-wrap: wrap
    width: (100% - 2px)
    height: 800px
    @media desktopscreen
        height: 1231px
    li
        overflow: hidden
        .preview
            transition: transform 0.5s ease
            height: 100%
            width: 100%
            background-position: center
            background-size: cover
            cursor: pointer
    li:hover
        .preview
            transform: scale(1.2)
    #list-item1
        height: 200px
        width: 60%
        @media desktopscreen
            height: 300px
    #list-item2
        height: 200px
        width: 40%
        @media desktopscreen
            height: 300px
    #list-item3
        height: 200px
        width: 40%
        @media desktopscreen
            height: 300px
    #list-item4
        height: 200px
        width: 60%
        @media desktopscreen
            height: 300px
    #list-item5
        height: 200px
        width: 60%
        @media desktopscreen
            height: 300px
    #list-item6
        height: 200px
        width: 40%
        @media desktopscreen
            height: 300px
    #list-item7
        height: 200px
        width: 40%
        @media desktopscreen
            height: 331px
    #list-item8
        height: 200px
        width: 60%
        @media desktopscreen
            height: 331px

    #gallery-image1
        background-image: url("/img/gallery-1.jpg")
    #gallery-image2
        background-image: url("/img/gallery-2.jpg")
    #gallery-image3
        background-image: url("/img/gallery-3.jpg")
    #gallery-image4
        background-image: url("/img/gallery-4.jpg")
    #gallery-image5
        background-image: url("/img/gallery-5.jpg")
    #gallery-image6
        background-image: url("/img/gallery-6.jpg")
    #gallery-image7
        background-image: url("/img/gallery-7.jpg")
    #gallery-image8
        background-image: url("/img/gallery-8.jpg")
        background-position: top

